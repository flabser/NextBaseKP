<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="grantaccess">
	<description>Доступ на ознакомление</description>
    <rununderuser source="macro">CURRENT_USER</rununderuser>
	<caching>NO_CACHING</caching>
	<element mode="ON" type="SCRIPT">
		<name>script_result</name>
		<events>
			<doscript source="file">page.grantaccess.DoScript</doscript>
		</events>
	</element>
</rule>